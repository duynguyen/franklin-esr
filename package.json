{
  "name": "franklin-esr",
  "scripts": {
    "dev": "node ./bin/franklin.js dev",
    "serve:node": "node ./bin/franklin.js serve:node",
    "serve:worker": "wrangler dev",
    "build:worker": "npm run build:client:worker && npm run esbuild:worker",
    "build:client:worker": "node ./bin/franklin.js build:client:worker",
    "build:node": "node ./bin/franklin.js build:node",
    "esbuild:worker": "node ./bin/franklin.js esbuild:worker",
    "prod:node": "npm run build:node && npm run serve:node",
    "dev:worker": "npm run build:worker && npm run serve:worker",
    "publish": "node ./bin/franklin.js publish"
  },
  "bin": {
    "franklin": "bin/franklin.js"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@vitejs/plugin-react": "^1.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "estree-walker": "^2.0.2",
    "express": "^4.17.3",
    "magic-string": "^0.26.2",
    "node-fetch": "^3.2.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vite": "^2.8.4",
    "vite-plugin-ssr": "^0.3.62",
    "wrangler": "^2.0.3"
  }
}
