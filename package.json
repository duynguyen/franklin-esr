{
  "scripts": {
    "dev": "node ./server/index.mjs",
    "serve:node": "cross-env NODE_ENV=production node ./server/index.mjs",
    "serve:worker": "wrangler pages dev ./dist/client/ --kv MODELS PAGES CSS",
    "build:worker": "npm run build:client:worker && npm run esbuild:worker",
    "build:client:worker": "vite build && vite build --ssr --mode worker",
    "build:node": "vite build && vite build --ssr",
    "esbuild:worker": "node -r esbuild-register build-worker.js",
    "prod:node": "npm run build:node && npm run serve:node",
    "prod:worker": "npm run build:worker && npm run serve:worker"
  },
  "dependencies": {
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@vitejs/plugin-react": "^1.2.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "express": "^4.17.3",
    "node-fetch": "^3.2.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vite": "^2.9.9",
    "vite-plugin-ssr": "^0.3.62",
    "wrangler": "^2.0.3"
  }
}
